#include <stdio.h>
int main(){
    int a[10], i, n, sum;
    printf("Enter array size: \n");
    scanf("%d", &n);
    printf("Enter %d elements: ", n);
    for (i=0; i<n; i++){
        scanf("%d", &a[i]);
    }
    printf("Displaying array: \n");
    for (i=0; i<n; i++){
        printf("%d\t", a[i]);
    }
    for (i=0; i<n; i++){
      sum = sum + a[i];
    }
    printf("\nSum of array elem: %d", sum);
}