#include <stdio.h>
//if we don't return any value, function type should be void

int find_max(int, int);
int main(){
    int x, y;
    printf("Enter two integers: \n");
    scanf("%d %d", &x, &y);
    printf("Max is: %d", find_max(x, y));
}
int find_max(int x, int y){
    int max;
    if (x>y){
        max=x;
    } else {
        max=y;
    }
    return(max);
}