#include <stdio.h>
int main(){
    int x, y, z, max;

    printf("Enter three integer values: \n");
    scanf("%d %d %d", &x, &y, &z);

    if (x>y && x>z){
        max = x;
    } else if (y>x && y>z){
        max = y;
    } else {
        max = z;
    }
    printf("Max = %d", max);
    }
